#
# pkg repository location
#
Alias /pkgs/ <%= @pkgRoot %>/
<Directory <%= @pkgRoot %>>
   Options Indexes
</Directory> 

ScriptAliasMatch ^/options/([^/]*)/(.*) /var/rundeck/projects/$1/jobs.d/options/$2
<Directory /var/rundeck/projects/*/jobs.d/options>
   Options +ExecCGI
   AddHandler cgi-script .options
</Directory>
